<!-- Start Hero -->
<section id="hero" ng-controller="indexController">
  <div id="gradient"></div>
  <div class="container">
    <div class="row">
      <div class="hero-text">
        <div class="col-md-6">
          <h3 class="title">ARE YOU READY FOR TAKE OFF?</h3>
          <p class="lead">
            eBusiness, eHealth, Document Scanning, POS and Cloud Solutions
          </p>
          <ul class="features-list custom-list">
            <li><i class="fa fa-check"></i><span>MERCHANT TRANSACTIONS</span></li>
